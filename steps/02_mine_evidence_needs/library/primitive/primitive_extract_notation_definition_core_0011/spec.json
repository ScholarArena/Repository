{
  "action": "accept",
  "skip_reason": "",
  "kind": "primitive",
  "name": "Extract_Notation_Definition",
  "need_summary": "Find notation definitions for a symbol.",
  "spec": {
    "goal": "Locate sentences defining a symbol.",
    "inputs": {
      "context": "dict with segments",
      "params": "dict with symbol"
    },
    "outputs": {
      "type": "text",
      "payload": {
        "symbol": "str",
        "matches": "list"
      },
      "prov": "list[int]"
    },
    "constraints": [
      "deterministic",
      "no network"
    ]
  },
  "tests": [
    {
      "name": "test_notation_found",
      "context": {
        "segments": [
          {
            "id": 1,
            "text": "Let x denote the input feature vector."
          }
        ]
      },
      "params": {
        "symbol": "x"
      },
      "expected": {
        "status": "ok",
        "prov_contains": [
          1
        ],
        "payload_keys": [
          "matches",
          "symbol"
        ]
      }
    },
    {
      "name": "test_notation_missing",
      "context": {
        "segments": [
          {
            "id": 2,
            "text": "No symbols here."
          }
        ]
      },
      "params": {
        "symbol": "z"
      },
      "expected": {
        "status": "missing",
        "payload_keys": [
          "matches",
          "symbol"
        ]
      }
    }
  ],
  "dependencies": []
}