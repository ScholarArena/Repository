{
  "action": "accept",
  "skip_reason": "",
  "kind": "primitive",
  "name": "Extract_Section",
  "need_summary": "Locate a section by title.",
  "spec": {
    "goal": "Find segments matching a section title.",
    "inputs": {
      "context": "dict with segments list of {id:int, text:str}",
      "params": "dict with title string"
    },
    "outputs": {
      "type": "text",
      "payload": {
        "title": "str",
        "matches": "list"
      },
      "prov": "list[int]"
    },
    "constraints": [
      "deterministic",
      "no network"
    ]
  },
  "tests": [
    {
      "name": "test_section_found",
      "context": {
        "segments": [
          {
            "id": 1,
            "text": "3 Results\nWe report..."
          }
        ]
      },
      "params": {
        "title": "Results"
      },
      "expected": {
        "status": "ok",
        "prov_contains": [
          1
        ],
        "payload_keys": [
          "matches",
          "title"
        ]
      }
    },
    {
      "name": "test_section_missing",
      "context": {
        "segments": [
          {
            "id": 2,
            "text": "Introduction text."
          }
        ]
      },
      "params": {
        "title": "Conclusion"
      },
      "expected": {
        "status": "missing",
        "payload_keys": [
          "matches",
          "title"
        ]
      }
    }
  ],
  "dependencies": []
}