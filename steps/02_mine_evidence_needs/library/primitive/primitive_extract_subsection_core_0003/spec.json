{
  "action": "accept",
  "skip_reason": "",
  "kind": "primitive",
  "name": "Extract_Subsection",
  "need_summary": "Locate a subsection by title.",
  "spec": {
    "goal": "Find segments matching a subsection title.",
    "inputs": {
      "context": "dict with segments list",
      "params": "dict with title string"
    },
    "outputs": {
      "type": "text",
      "payload": {
        "title": "str",
        "matches": "list"
      },
      "prov": "list[int]"
    },
    "constraints": [
      "deterministic",
      "no network"
    ]
  },
  "tests": [
    {
      "name": "test_subsection_found",
      "context": {
        "segments": [
          {
            "id": 1,
            "text": "3.2 Ablation Study\nDetails..."
          }
        ]
      },
      "params": {
        "title": "Ablation Study"
      },
      "expected": {
        "status": "ok",
        "prov_contains": [
          1
        ],
        "payload_keys": [
          "matches",
          "title"
        ]
      }
    },
    {
      "name": "test_subsection_missing",
      "context": {
        "segments": [
          {
            "id": 2,
            "text": "4 Experiments"
          }
        ]
      },
      "params": {
        "title": "Notation"
      },
      "expected": {
        "status": "missing",
        "payload_keys": [
          "matches",
          "title"
        ]
      }
    }
  ],
  "dependencies": []
}